{
  "sandbox": {
    "app_function": "wsgi_rule_engine.application",
    "apigateway_description": "simple serverless rule engine",
    "api_key_required": true,
    "aws_region": "ap-south-1",
    "profile_name": "sandbox",
    "project_name": "rule_engine",
    "runtime": "python3.6",
    "s3_bucket": "zappa-rule-engine-sandbox",
    "environment_variables": {
      "env": "sandbox"
    },
    "events": [],
    "extra_permissions": [
      {
        "Effect": "Allow",
        "Action": [
          "secretsmanager:*"
        ],
        "Resource": "*"
      }
    ]
  }
}