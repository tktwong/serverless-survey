{
    "dev": {
        "aws_region": "eu-west-1",
        "app_function": "server.app",
        "s3_bucket": "zappa-dog-dailystandup-dev",
        "keep_warm": false,
        "parameter_depth": 2,
        "memory_size": 128,
        "exclude": ["awscli*","static","s3cmd*"],
        "profile_name": "daily-standup",
        "method_header_types": [
          "Content-Type",
          "Location",
          "Status",
          "X-Frame-Options",
          "Set-Cookie",
          "Access-Control-Allow-Origin",
          "Access-Control-Allow-Headers",
          "Access-Control-Allow-Methods"
      ]
    },
    "live": {
        "aws_region": "eu-west-1",
        "app_function": "server.app",
        "s3_bucket": "zappa-dog-dailystandup-live",
        "keep_warm": false,
        "parameter_depth": 2,
        "memory_size": 512,
        "exclude": ["awscli*","static","s3cmd*"],
        "profile_name": "daily-standup",
        "method_header_types": [
          "Content-Type",
          "Location",
          "Status",
          "X-Frame-Options",
          "Set-Cookie",
          "Access-Control-Allow-Origin",
          "Access-Control-Allow-Headers",
          "Access-Control-Allow-Methods"
      ]
    }
}
